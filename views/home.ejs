<%- include('partial/header') %>

<body>
  <%- include("partial/nav") %>
  <div class="date">
    <h1>Enter Log</h1>
  </div>
  <div class="form-con">
    <form action="/" method="post">
      <div class="items">
        <label>Customer Name:</label>
        <input type="text" name="customerName" id="item" required>
      </div>
      <div class="items">
        <label>Address:</label>
        <input type="text" name="address" id="item" required>
      </div>
      <div class="items">
        <label>Tel:</label>
        <input type="tel" name="tel" id="item" required>
      </div>
      <div class="items">
        <label>Date:</label>
        <label><%=date%></label>
      </div>
      <div class="items">
        <label>Bill No:</label>
        <input type="text" name="billNo" id="item" required>
      </div>
      <div class="items">
        <label>Amount:</label>
        <input type="text" name="amount" id="item">
      </div>
      <div class="items">
        <label>Received:</label>
        <input type="text" name="received" id="item" required>
      </div>
      <div class="items">
        <label>Received Date:</label>
        <input type="date" name="receivedDate" id="item" required>
      </div>
      <div class="items">
        <button class="btn" type="submit">Submit</button>
      </div>
    </form>
  </div>

  <%- include('partial/footer') %>
</body>